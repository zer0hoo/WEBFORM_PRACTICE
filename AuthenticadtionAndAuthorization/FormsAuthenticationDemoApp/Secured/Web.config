<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
	  <authorization>
		  <deny users="?"/> <!-- anonimous한 유저는 deny -->
		  <deny roles="r1" verbs="Post"/> <!-- verb : request가 post인지 get인지에 따라 적용 여부 결정-->
		  <allow users="*"/> <!-- 검증이 된 유저는 모두 허용 -->
	  </authorization>
  </system.web>
</configuration>
